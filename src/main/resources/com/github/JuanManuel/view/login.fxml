<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="anchorPane" prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #a8ffb2;" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.JuanManuel.view.LoginController">
    <children>
        <Button fx:id="loginButton" layoutX="359.0" layoutY="410.0" mnemonicParsing="false" onAction="#onLogin" prefHeight="50.0" prefWidth="100.0" style="-fx-background-color: #044004;" text="Entrar" textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font size="24.0" />
            </font>
        </Button>
        <Button fx:id="goToRegisterButton" layoutX="441.0" layoutY="518.0" mnemonicParsing="false" onAction="#goToRegister" style="-fx-background-color: transparent;" text="Registrarse" textFill="#044004" underline="true">
         <font>
            <Font size="18.0" />
         </font></Button>
        <Text layoutX="195.0" layoutY="545.0" strokeType="OUTSIDE" strokeWidth="0.0" text="¿No tienes una cuenta?">
         <font>
            <Font size="18.0" />
         </font></Text>
        <Text fill="#044004" layoutY="167.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Iniciar Sesión" textAlignment="CENTER" wrappingWidth="800.0">
            <font>
                <Font size="72.0" />
            </font>
        </Text>
        <TextField fx:id="phoneField" layoutX="356.0" layoutY="229.0" prefHeight="50.0" prefWidth="300.0">
            <font>
                <Font size="24.0" />
            </font>
        </TextField>
        <TextField fx:id="passwordField" layoutX="356.0" layoutY="314.0" prefHeight="50.0" prefWidth="300.0">
            <font>
                <Font size="24.0" />
            </font>
        </TextField>
      <Text fx:id="phoneText" fill="#044004" layoutX="195.0" layoutY="264.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Teléfono:">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Text>
      <Text fx:id="passwordText" fill="#044004" layoutX="181.0" layoutY="349.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Contraseña:">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Text>
    </children>
</AnchorPane>
